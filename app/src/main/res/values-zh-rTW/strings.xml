	<!-- Settings -->
	<string name="title_activity_settings">設定</string>

	<string name="pref_title_file_exists">如果檔案已經存在的行為</string>
	<array name="pref_list_title_file_exists">
		<item>覆寫已存在的檔案</item>
		<item>不儲存</item>
		<item>在名稱後附加數字</item>
		<!--<item>Request new name</item>-->
	</array>
	<array name="pref_list_description_file_exists">
		<item>以新檔案覆寫已存在的檔案</item>
		<item>不儲存新檔案</item>
		<item>在檔案名稱後附加數字，使兩個檔案都儲存</item>
		<!--<item>Request new file name with a pop up</item>-->
	</array>
	<string name="pref_default_value_file_exists">覆寫</string>
